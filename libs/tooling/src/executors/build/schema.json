{
  "$schema": "http://json-schema.org/schema",
  "version": 2,
  "cli": "nx",
  "title": "Build executor",
  "description": "",
  "type": "object",
  "properties": {
    "outputPath": {
      "type": "string",
      "description": "The output path for the built extension"
    },
    "target": {
      "type": "string",
      "enum": ["firefox"],
      "description": "The target browser to build the extension for"
    }
  },
  "required": ["target", "outputPath"]
}
