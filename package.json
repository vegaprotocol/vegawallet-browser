    "is-mobile": "^4.0.0",
    "javascript-time-ago": "^2.5.9",
    "lodash": "^4.17.21",
    "mutexify": "^1.4.0",
    "nanoassert": "^2.0.0",
    "object-hash": "^3.0.0",
    "protobuf-codec": "^1.0.6",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.39.1",
    "react-router-dom": "^6.10.0",
    "react-time-ago": "^7.2.1",
