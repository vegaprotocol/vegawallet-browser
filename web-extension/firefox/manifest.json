{
  "manifest_version": 2,
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "fairground.png",
      "48": "fairground.png",
      "128": "fairground.png"
    },
    "default_title": "Minimal Extension",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "./static/js/background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'wasm-unsafe-eval'",
  "web_accessible_resources": [
    "/static/js/in-page.js"
  ]
}
